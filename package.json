{
  "name": "vite-template",
  "author": "Jo√£o Gabriel <main.joaogabrielsg@gmail.com>",
  "description": "A template for React projects using Vite and TypeScript.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup:project": "pnpm install && pnpm lefthook install",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typeCheck": "tsc",
    "test:unit": "vitest src/ --run --passWithNoTests",
    "test:unit:watch": "vitest src/",
    "test:unit:coverage": "vitest src/ --run --passWithNoTests --coverage",
    "test:unit:coverage:watch": "vitest src/ --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^9.0.3",
    "@playwright/test": "^1.47.2",
    "@tanstack/eslint-plugin-query": "^5.59.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/eslint-plugin": "^1.1.4",
    "@vitest/ui": "^2.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^3.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "lefthook": "^1.7.17",
    "msw": "^2.4.9",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  }
}